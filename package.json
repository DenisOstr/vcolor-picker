{
  "name": "@envis/vcolor-picker",
  "description": "Vue 3 + Vite color picker. Simple and gradient color picker.",
  "private": false,
  "version": "1.1.0",
  "author": {
    "name": "Denis Ostrovsky",
    "email": "denisostrovskis1@gmail.com"
  },
  "license": "MIT",
  "repository": "https://github.com/DenisOstr/vcolor-picker",
  "files": [
    "dist/*",
    "src/*",
    "public/*",
    "*.json",
    "*.js"
  ],
  "keywords": [
    "Vue 3",
    "Vite",
    "TailwindCSS",
    "Color picker",
    "Gradient color picker",
    "Composition API"
  ],
  "type": "module",
  "main": "./dist/vcolor-picker.umd.js",
  "module": "./dist/vcolor-picker.es.js",
  "exports": {
    ".": {
      "import": "./dist/vcolor-picker.es.js",
      "require": "./dist/vcolor-picker.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.37",
    "vuex": "^4.0.2",
    "vuex-persist": "^3.1.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "autoprefixer": "^10.4.8",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "vite": "^3.0.7"
  }
}
